@model Department
@{
    ViewData["Title"] = "Edit";
}

<h1>Edit @Model.Name Department</h1>
<div asp-validation-summary="All">

</div>
<form asp-action="Edit">

    <div class="form-group">
        <label class="col-form-label" asp-for="Code">

        </label>
        <input class="form-control" asp-for="Code" />
        <span asp-validation-for="Code" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label class="col-form-label" asp-for="Name">

        </label>
        <input class="form-control" asp-for="Name" />
        <span asp-validation-for="Name" class="text-danger"></span>
    </div>
    <div class="form-group mb-3">
        <label class="col-form-label" asp-for="CreatedDate">

        </label>
        <input class="form-control" asp-for="CreatedDate" />
        <span asp-validation-for="CreatedDate" class="text-danger"></span>
    </div>
    <input type="submit" name="name" value="Save" class="btn btn-primary" />
    <a asp-action="Index" class="btn btn-light"> Back to List</a>
</form>
